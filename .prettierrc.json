{
	"printWidth": 130,
		"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"jsxSingleQuote": false,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"insertPragma": false,
	"requirePragma": false,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "auto",
	"rangeStart": 0,
	"vueIndentScriptAndStyle": false
}
